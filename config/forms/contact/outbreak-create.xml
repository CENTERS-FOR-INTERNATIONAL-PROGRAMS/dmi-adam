          <value>measles</value>
        </item>
        <item>
          <label ref="jr:itext('/data/outbreak/code/covid19:label')"/>
          <value>covid19</value>
        </item>
      </select1>
      <input ref="/data/outbreak/description">
        <label ref="jr:itext('/data/outbreak/description:label')"/>
        <hint ref="jr:itext('/data/outbreak/description:hint')"/>
      </input>
      <input ref="/data/outbreak/location">
        <label ref="jr:itext('/data/outbreak/location:label')"/>
        <hint ref="jr:itext('/data/outbreak/location:hint')"/>
      </input>
      <input ref="/data/outbreak/date">
        <label ref="jr:itext('/data/outbreak/date:label')"/>
        <hint ref="jr:itext('/data/outbreak/date:hint')"/>
      </input>
      <select1 appearance="columns" ref="/data/outbreak/status">
        <label ref="jr:itext('/data/outbreak/status:label')"/>
        <hint ref="jr:itext('/data/outbreak/status:hint')"/>
        <item>
          <label ref="jr:itext('/data/outbreak/status/open:label')"/>
          <value>open</value>
        </item>
        <item>
          <label ref="jr:itext('/data/outbreak/status/closed:label')"/>
          <value>closed</value>
        </item>
      </select1>
      <group appearance="hidden" ref="/data/outbreak/meta"/>
    </group>
  </h:body>
