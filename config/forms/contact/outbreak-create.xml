    </group>
    <group appearance="field-list" ref="/data/outbreak">
      <select1 ref="/data/outbreak/code">
        <label ref="jr:itext('/data/outbreak/code:label')"/>
        <hint ref="jr:itext('/data/outbreak/code:hint')"/>
        <item>
          <label ref="jr:itext('/data/outbreak/code/unknown:label')"/>
          <value>unknown</value>
        </item>
        <item>
          <label ref="jr:itext('/data/outbreak/code/evd:label')"/>
